
alias sshvb='ssh -p 2222 bcoff@localhost'
alias scpvb='scp -P 2222 bcoff@localhost:'
alias s="source $HOME/.bash_profile"

scp2vb() {
    scp -P 2222 $1 bill@localhost:
    echo copy completed;
}

